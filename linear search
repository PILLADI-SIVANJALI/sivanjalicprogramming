#include<stdio.h>
main(){
	int x[6]={9,7,13,6,14,2};
	int key, i, found = 0, position;
	printf("enter the search element");
	scanf("%d",&key);
	for(i=0;i<=5;i++)
	{
		if(key=x[i])
		{
			found=1;
			position=i;
		}
	}
	if(found==1)
	printf("%d is found in %d locatoin",key,position);
	else
	printf("%d is not foound",key);
}
